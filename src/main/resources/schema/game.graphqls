type GameConnection implements Connection {
    edges: [GameEdge]
    pageInfo: PageInfo
    games: [Game!]!
    count: Int!
    totalCount: Int
}

type GameEdge implements Edge {
    node: Game!
    cursor: String
}

type Game implements Node{
    id: ID!
    bggId: Int
    name: String!
    metadata: GameMetadata!
}

input GameInput {
    bggId: Int
    name: String!
}
